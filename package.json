{
  "name": "@guesung/ui",
  "version": "0.0.0",
  "description": "React 19 컴포넌트 라이브러리 - Select와 Modal 컴포넌트를 제공하는 TypeScript 기반 UI 라이브러리",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "react-components",
    "typescript",
    "tailwindcss",
    "select",
    "modal",
    "ui-library",
    "component-library",
    "compound-components",
    "accessibility",
    "a11y"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/guesung/thedotscorp-frontend-assign.git"
  },
  "homepage": "https://github.com/guesung/thedotscorp-frontend-assign#readme",
  "bugs": {
    "url": "https://github.com/guesung/thedotscorp-frontend-assign/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/browser-playwright": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "4.0.13",
    "chromatic": "^13.3.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^10.0.8",
    "globals": "^16.4.0",
    "jsdom": "^25.0.1",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.53.0",
    "storybook": "^10.0.8",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^4.0.13"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "engines": {
    "node": "22.17.0"
  }
}
